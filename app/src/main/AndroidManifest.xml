<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mvvmapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".LeakCanaryApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon, android:label,android:theme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".myinfo.MyInfoActivity" />
        <activity android:name=".myshow.MyShowActivity" />
        <activity android:name=".mylist.MyListActivity" />
        <activity android:name=".mychat.MyChatActivity" />
        <activity android:name=".myFans.MyFansActivity" />
    </application>

</manifest>